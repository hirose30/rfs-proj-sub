{
  "name": "imp-log-etl",
  "version": "1.0.0",
  "description": "BigQuery impression log ETL service",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathIgnorePatterns=integration",
    "test:integration": "RUN_INTEGRATION_TESTS=true jest --testPathPattern=integration",
    "lint": "eslint . --ext .ts",
    "deploy:dev": "bash scripts/deploy/deploy.sh dev",
    "deploy:staging": "bash scripts/deploy/deploy.sh staging",
    "deploy:prod": "bash scripts/deploy/deploy.sh prod",
    "deploy:dev:ps": "powershell -ExecutionPolicy Bypass -File scripts/deploy-ps/Deploy.ps1 dev",
    "deploy:staging:ps": "powershell -ExecutionPolicy Bypass -File scripts/deploy-ps/Deploy.ps1 staging",
    "deploy:prod:ps": "powershell -ExecutionPolicy Bypass -File scripts/deploy-ps/Deploy.ps1 prod",
    "docker:build": "docker build -t imp-log-etl:latest ."
  },
  "dependencies": {
    "@google-cloud/bigquery": "^6.2.1",
    "@google-cloud/storage": "^7.15.2",
    "@types/uuid": "^10.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "google-auth-library": "^9.15.1",
    "uuid": "^11.1.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.5",
    "eslint": "^8.46.0",
    "jest": "^29.6.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
